{
  "manifest_version": 2,

  "name": "E-Dom Enhancer",
  "description": "Adds new capabilities to the online Dominion game.",
  "version": "0.0.0.3",

  "content_scripts": [
    {
      "matches":["https://dominion.games/*", "https://www.dominion.games/*"],
      "js":["js/main.js"]
    }
  ],

  "permissions": ["storage"],

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
